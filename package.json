{
  "name": "tvii-us",
  "version": "3.0.0",
  "description": "The second rewrite of Nintendo TVii. This time in Node!",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "license": "ISC",
  "homepage": "https://projectrose.cafe/",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist/ && rm -rf .tsbuildinfo && npm run lint && tsc",
    "lint": "eslint --fix .",
    "start": "npm install && npm run build && node dist/index.js",
    "dev": "nodemon --watch src --ext ts,json --exec \"tsc && node dist/index.js\""
  },
  "engines": {
    "node": "v23.3.0",
    "npm": "10.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Project-Rose/tvii.git"
  },
  "keywords": [
    "nintendo",
    "revival",
    "tvii",
    "wiiu"
  ],
  "authors": [
    "David J",
    "SkyeDoesThings",
    "ItzSwirlz",
    "Mig",
    "Cooper"
  ],
  "bugs": {
    "url": "https://github.com/Project-Rose/tvii/issues"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.11.0",
    "@stylistic/eslint-plugin-migrate": "^2.11.0",
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "globals": "^15.12.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt-updated": "^5.0.16",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.13",
    "express": "^4.21.2",
    "mysql2": "^3.11.5",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.5",
    "typeorm": "^0.3.20"
  }
}
